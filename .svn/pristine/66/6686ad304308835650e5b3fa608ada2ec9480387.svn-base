public class BranchActInfoBean {

    private String branchid = "";  // 机构号
    private String actbegin = "";  // 活动开始日期
    private String actend = "";  // 活动结束日期
    private String limit = "";  // 兑换上限
    private String status = "";  // 状态

    public String getBranchid() {
        return branchid;
    }

    public void setBranchid(String branchid) {

        if (branchid != null) {
            this.branchid = branchid.trim();
        } else {
            this.branchid = "";
        }
    }

    public String getActbegin() {
        return actbegin;
    }

    public void setActbegin(String actbegin) {

        if (actbegin != null) {
            this.actbegin = actbegin.trim();
        } else {
            this.actbegin = "";
        }
    }

    public String getActend() {
        return actend;
    }

    public void setActend(String actend) {

        if (actend != null) {
            this.actend = actend.trim();
        } else {
            this.actend = "";
        }
    }

    public String getLimit() {
        return limit;
    }

    public void setLimit(String limit) {

        if (limit != null) {
            this.limit = limit.trim();
        } else {
            this.limit = "";
        }
    }

    public String getStatus() {
        return status;
    }

    public void setStatus(String status) {

        if (status != null) {
            this.status = status.trim();
        } else {
            this.status = "";
        }
    }
}
